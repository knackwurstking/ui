package app

import (
	"fmt"
	"github.com/knackwurstking/ui/pkg/css"
)

const (
	IDSectionText = "section-text"
)

templ SectionText() {
	<section
		style={ fmt.Sprintf("padding-top: %s", css.VarAppBarHeight) }
		class={ css.P, css.MT }
	>
		@SectionTitle() {
			Text: H1-6
		}
		@SectionContent() {
			<!-- Headings -->
			<h1>Heading 1</h1>
			<h2>Heading 2</h2>
			<h3>Heading 3</h3>
			<h4>Heading 4</h4>
			<h5>Heading 5</h5>
			<h6>Heading 6</h6>
		}
	</section>
}
